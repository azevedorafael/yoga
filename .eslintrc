{
  "parser": "babel-eslint",
  "plugins": ["react", "prettier"],
  "extends": [
    "airbnb",
    "prettier",
    "prettier/react",
    "plugin:prettier/recommended"
  ],
  "rules": {
    "prettier/prettier": "error",
    "import/no-cycle": 0,
    "react/jsx-props-no-spreading": "off"
  },
  "globals": {
    "window": true
  }
}
